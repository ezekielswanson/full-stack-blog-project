{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["**/*.js", "**/*.ejs", "**/*.json"],
          "nodeVersion": "18.x"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
    "installCommand": "npm install --omit=dev"
  }